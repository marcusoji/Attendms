{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/courses/([a-zA-Z0-9]+)/stats",
      "dest": "/api/courses/[courseId]/stats.js"
    },
    {
      "src": "/api/courses/sessions",
      "dest": "/api/courses/sessions.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}